"                                                </td>\n"
"                                                <td>\n"
"                                                    <a t-attf-href=\"/rate/"
"{{ access_token }}/1\">\n"
"                                                        <img alt=\"Unhappy\" "
"src=\"/rating/static/src/img/rating_1.png\" title=\"Unhappy\"/>\n"
"                                                    </a>\n"
